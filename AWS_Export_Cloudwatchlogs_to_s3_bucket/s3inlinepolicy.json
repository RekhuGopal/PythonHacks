{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "uploadlogs",
            "Effect": "Allow",
            "Principal": {
                "Service": "logs.us-east-1.amazonaws.com"
            },
            "Action": "s3:GetBucketAcl",
            "Resource": "arn:aws:s3:::logexportbucketdemo"
        },
        {
            "Sid": "uploadlogs",
            "Effect": "Allow",
            "Principal": {
                "Service": "logs.us-east-1.amazonaws.com"
            },
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::logexportbucketdemo/*"
        }
    ]
}